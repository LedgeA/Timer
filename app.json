{
  "expo": {
    "name": "Timer",
    "slug": "Timer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/timer-adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/timer-splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#002147"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/timer-adaptive-icon.png"
      },
      "package": "com.ledgea.Timer"
    },
    "web": {
      "favicon": "./assets/images/timer-splash-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3bd0bca4-5492-47c5-8a5e-fffcaa4c9841"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": 
          [
            "./assets/fonts/Georama-Bold.ttf",
            "./assets/fonts/Georama-Regular.ttf",
            "./assets/fonts/Georama-Medium.ttf"
          ]
        }
      ]
    ]
  }
}
